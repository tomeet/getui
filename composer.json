{
    "name": "tomeet/getui",
    "description": "getui for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomeet",
            "email": "tomeet@sohu.com",
            "homepage": ""
        }
    ],
    "homepage": "",
    "keywords": ["Laravel", "GeTui"],
    "require": {
        "illuminate/support": "~5.5|~6.0|~7.0|~8.0",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "psr/simple-cache": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Tomeet\\GeTui\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tomeet\\GeTui\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tomeet\\GeTui\\GeTuiServiceProvider"
            ],
            "aliases": {
                "GeTui": "Tomeet\\GeTui\\Facades\\GeTui"
            }
        }
    },
        "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
